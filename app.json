{
  "expo": {
    "name": "Penny - Portfolio Tracker",
    "slug": "clearpath-financial-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-square.png",
    "scheme": "penny",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0E17"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.penny.financialapp",
      "usesAppleSignIn": true,
      "icon": "./assets/images/icon-square.png",
      "splash": {
        "image": "./assets/images/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#0A0E17"
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.114545191096-h3jf6qkljg4c90jjrojpm47a4frjkbnk"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["fetch", "processing"],
        "BGTaskSchedulerPermittedIdentifiers": [
          "PORTFOLIO_PRICE_REFRESH"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-square.png",
        "backgroundColor": "#0A0E17"
      },
      "splash": {
        "image": "./assets/images/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#0A0E17"
      },
      "package": "com.penny.financialapp",
      "permissions": [
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-apple-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#064E3B"
        }
      ],
      "expo-background-fetch",
      "expo-task-manager"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://rork.com/"
      },
      "eas": {
        "projectId": "13bb7bb4-2e9b-42d8-8c09-cc43c3337077"
      }
    }
  }
}
